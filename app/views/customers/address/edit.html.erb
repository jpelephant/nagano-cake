<h1><strong>配送先編集</strong></h1>
<div class="row">
  <div class="col-md-10">
    <%= from_with model: @address local ture do |f| %>
    <table>
      <tr>
        <th>郵便番号（ハイフンなし）</th>
        <td><%= f.text_field :postal_code %></td>
      </tr>
      <tr>
        <th>住所</th>
        <td><%= f.text_field :address %></td>
      </tr> 
      <tr>
        <th>宛名</th>
        <td><%= f.text_field :name %></td>
        <td><%= f.submit "変更内容を保存" %></td>
      </tr>
    </table>
  </div>
</div>
